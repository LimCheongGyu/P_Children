<title>header</title>
<style type="text/css">
* {
	margin: 0;
}
.wrap{
	width: 1000px;
	margin: auto;
	text-align: center;
}
.header {
	width: 1000px;
}
.title {
	font-size: 50px;
	text-align: center;
	margin-top: 40px;
	padding-bottom: 20px;
}
.navdiv {
	width: 100%;
}
nav {
	width: 100%;
	background-color: olive;
}
nav ul {
	list-style: none;
	display: flex;
	justify-content: end;
}
nav ul li {
	padding: 10px;
}
nav ul li a {
	text-decoration: none;
	color: white;
}
nav ul li a:hover {
	color: orange;
}
a { 
	text-decoration:none;
	color : black;
}
